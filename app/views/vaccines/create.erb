<h1>Create Vaccine</h1>
<form action="/vaccines/new" method="post">
  <lable>Name:</lable>
  <input type="text" name="name" ></input><br>
  <lable>Disease:</lable>
  <input type="text" name="disease" ></input><br>
  <lable>Time:</lable>
  <input type="text" name="time" ></input><br>
  <% @patients.each do |patient|%>
    <p>Patient:<%= patient.full_name%></p>
    <input type="radio" name="patient" id=<%patient.id %>></input>
    <br>
  <%end%>
  <input type="submit"></input>
</form>