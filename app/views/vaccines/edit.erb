<h1>Edit Vaccine</h1>
<form action="/vaccines/edit/<%=@vaccine.id%>" method="post">
  <input id="hidden" type="hidden" name="_method" value="PATCH"> #Mysterious hidden input for PATCH
  <lable>Name:</lable>
  <input type="text" name="name" value=<%=@vaccine.name%>></input><br>
  <lable>Disease:</lable>
  <input type="text" name="disease" value=<%=@vaccine.disease%>></input><br>
  <lable>Time:</lable>
  <input type="text" name="time" value=<%=@vaccine.time%>></input><br>
  <% @patients.each do |patient|%>
    <p>Patient:<%= patient.full_name%></p>
    <input type="radio" name="patient" id=<%patient.id %>></input>
    <br>
  <%end%>
  <input type="submit"></input>
</form>